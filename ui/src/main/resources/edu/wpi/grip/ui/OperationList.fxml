<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.VBox?>
<Tab xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.grip.ui.OperationListController" fx:id="root"
     closable="false">
    <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="AS_NEEDED"
                VBox.vgrow="ALWAYS">
        <VBox fx:id="operations" styleClass="operation-list"/>
    </ScrollPane>
</Tab>
