<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<fx:root styleClass="socket" type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/null"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="identifier" contentDisplay="RIGHT" styleClass="socket-identifier" GridPane.rowIndex="0"
               GridPane.columnIndex="1">
            <graphic>
                <Label fx:id="type" opacity=".5"/>
            </graphic>
        </Label>
        <StackPane fx:id="contentPane" styleClass="socket-control" GridPane.rowIndex="1"
                   GridPane.columnIndex="1"/>
    </children>
</fx:root>
